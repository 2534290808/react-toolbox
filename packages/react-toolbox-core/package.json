{
  "name": "react-toolbox-core",
  "version": "0.0.0-alpha.0",
  "description": "A set of factories to generate universal agnostic components",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./lib --ignore '__tests__'",
    "build:watch": "babel ./src --out-dir ./lib --ignore '__tests__' --watch",
    "build:ts": "tsc",
    "build:ts:watch": "tsc --watch",
    "build:docs": "typedoc --out ./docs",
    "prepublish": "yarn run build",
    "test": "node --harmony_proxies node_modules/.bin/jest --maxWorkers 4",
    "test:watch": "node --harmony_proxies node_modules/.bin/jest --watch"
  },
  "keywords": [],
  "author": "Javi Velasco <javier.velasco86@gmail.com> (http://javivelasco.com/)",
  "license": "ISC",
  "dependencies": {
    "date-fns": "^1.28.4",
    "inline-style-prefixer": "^3.0.3",
    "ramda": "^0.23.0"
  },
  "peerDependencies": {
    "react": "^0.14 || ~15.4.0",
    "react-dom": "^0.14.0 || ~15.4.0"
  },
  "devDependencies": {
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^19.2.3",
    "@types/ramda": "^0.0.8",
    "@types/react": "^15.0.24",
    "babel-cli": "^6.24.1",
    "typedoc": "^0.6.0",
    "typescript": "next"
  }
}
