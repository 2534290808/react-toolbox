{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react",
    "noImplicitAny": false,
    "outDir": "lib",
    "module": "commonjs",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "__tests__"
  ]
}
